package Insurance;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.testng.Assert;
import org.testng.annotations.Parameters;
import org.testng.annotations.Test;
import org.testng.asserts.SoftAssert;

import io.github.bonigarcia.wdm.WebDriverManager;

public class TestNGDP {
	
	@Parameters({"URL","Username","Password"})
	@Test
	public void logintoSwaglabs(String abc,String usnmae,String pwd) throws InterruptedException
	{
		WebDriver driver = WebDriverManager.chromedriver().create();
		driver.manage().window().maximize();
		driver.get(abc);
		driver.findElement(By.id("user-name")).click();
		driver.findElement(By.id("user-name")).sendKeys(usnmae);
		driver.findElement(By.id("password")).click();
		driver.findElement(By.id("password")).sendKeys(pwd);
		boolean act = driver.findElement(By.id("login-button")).isDisplayed();
		Thread.sleep(3000);		
		SoftAssert asert = new SoftAssert();
		asert.assertEquals(act, false);
		//Assert.assertEquals(act, false);
		System.out.println("ABC");
		System.out.println("ABC");
		
	}
	
	/*
	 * @DataProvider public Object[][] getlogin() { Object[][] data = new
	 * Object[4][2]; data[0][0]="standard_user"; data[0][1]="secret_sauce";
	 * data[1][0]="locked_out_user"; data[1][1]="secret_sauce";
	 * data[2][0]="problem_user"; data[2][1]="secret_sauce";
	 * data[3][0]="performance_glitch_user"; data[3][1]="secret_sauce"; return data;
	 * }
	 */


}
